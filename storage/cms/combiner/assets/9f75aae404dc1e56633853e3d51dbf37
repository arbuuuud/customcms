jQuery(document).ready(function($){
	var winWidth = $(window).width();
	if (winWidth > 767) {
		$('.header-menu').scrollToFixed({
			preFixed: function() { $(this).css('background-color', '#4f402f'); },
			postFixed: function() { $(this).css('background-color', 'rgba(79, 64, 47, 0.8)'); }
		});
	}
});

